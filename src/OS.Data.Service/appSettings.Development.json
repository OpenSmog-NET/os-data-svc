{
  "ConnectionStrings": {
    "AzureStorage": "UseDevelopmentStorage=true;DevelopmentStorageProxyUri=http://127.0.0.1:10002/",
    "Database": "Server=127.0.0.1;Port=5432;Database=os-measurements-dev;User Id=postgres;Password=postgres;"
  },

  "OS": {
    "WebJob": {
      "UseTimers": false,
      "DashboardConnectionStringName": "AzureStorage",
      "StorageConnectionStringName": "AzureStorage"
    }
  },

  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Logentries",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm} [{Application}] [{Stage}] [{Level}] {MachineName} {CorrelationId} {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Console"
      }
    ],

    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId" ],

    "Properties": {
      "Application": "os-smog-api",
      "Stage": "DEV"
    }
  }
}