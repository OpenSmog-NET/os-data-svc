{
  "OS": {
    "WebJob": {
      "UseTimers": false,
      "DashboardConnectionStringName": "AzureWebJobsDashboard",
      "StorageConnectionStringName": "AzureStorage"
    }
  },

  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "AzureLogAnalytics"
      },
      {
        "Name": "Console"
      }
    ],

    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId" ],

    "Properties": {
      "Application": "os-data-svc",
      "Stage": "PROD"
    }
  }
}