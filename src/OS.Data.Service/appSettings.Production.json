{
  "OS": {
    "WebJob": {
      "UseTimers": false,
      "DashboardConnectionStringName": "AzureStorage",
      "StorageConnectionStringName": "AzureWebJobStorage"
    }
  },

  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Logentries",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm} [{Application}] [{Stage}] [{Level}] {MachineName} {CorrelationId} {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Console"
      }
    ],

    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId" ],

    "Properties": {
      "Application": "os-data-svc",
      "Stage": "PROD"
    }
  }
}